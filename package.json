{
  "name": "ecommerce",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix fontend && npm run build --prefix frontend"
  },
  "author": "Artur Molenda",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "file-type": "^16.0.1",
    "fs": "0.0.1-security",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.13",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "react-helmet": "^6.1.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "morgan": "^1.10.0",
    "nodemon": "^2.0.6"
  },
